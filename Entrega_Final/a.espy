# EJEMPLO CALCULAR VALORES DE PI POR PRODUCTO DE WALLIS
FUNC PI()
SI(X==N) ENTONCES
FINSI

VAR IA = X * 2
VAR IB = X*2-1
VAR DB = X*2+1
VAR TOTAL = (IA/IB)*(IA/DB)
VAR X = X + 1
VAR PI = PI * TOTAL
SI(X < N) ENTONCES
PI()
FINSI
FINFUNC PI
VAR N = 1000
VAR X = 1
VAR PI = 1
PI()
VAR PI = PI * 2
IMPRIMIR("RES: ", PI)
FIN
